{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "0.1.0",
  "command": "gulp",
  "isShellCommand": true,
  "showOutput": "always",
  "suppressTaskName": false,
  "echoCommand": true,
  "tasks": [
    {
      "taskName": "benchmark"
    },
    {
      "taskName": "build",
      "isBuildCommand": true,
      "problemMatcher": "$tsc"
    },
    {
      "taskName": "capnp-compile"
    },
    {
      "taskName": "clean"
    },
    {
      "taskName": "coverage"
    },
    {
      "taskName": "lib-test"
    },
    {
      "taskName": "lint"
    },
    {
      "taskName": "test",
      "isTestCommand": true
    },
    {
      "taskName": "watch",
      "isBackground": true
    }
  ]
}
