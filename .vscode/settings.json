// Place your settings in this file to overwrite default and user settings.
{
  "files.exclude": {
    ".husky/**/_": true,
    ".nyc_output": true,
    "**/.DS_Store": true,
    "**/.eslintcache": true,
    "**/.git": true,
    "**/.grunt": true,
    "**/.hg": true,
    "**/.lock-wscript": true,
    "**/.node_repl_history": true,
    "**/.npm": true,
    "**/.nyc_output": true,
    "**/.svn": true,
    "**/.tmp": true,
    "**/*.log": true,
    "**/*.map": true,
    "**/*.pid.lock": true,
    "**/*.pid": true,
    "**/*.seed": true,
    "**/*.tgz": true,
    "**/*.tsbuildinfo": true,
    "**/coverage": true,
    "**/CVS": true,
    "**/jspm_packages": true,
    "**/lib-cov": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/node_modules/": true,
    "**/npm-debug.log*": true,
    "**/pids": true,
    "build/Release": true,
    "coverage": true,
    "packages/*/bin/**/*.d.ts": true,
    "packages/*/bin/**/*.js": true,
    "packages/*/src/**/*.d.ts": true,
    "packages/*/src/**/*.js": true,
    "packages/*/test/**/*.d.ts": true,
    "packages/*/test/**/*.js": true,
    "packages/js-examples/**/*.capnp.ts": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/lib": true,
    "**/lib-test": true,
    "packages/*/bin/**/*.d.ts": true,
    "packages/*/bin/**/*.js": true,
    "packages/*/src/**/*.d.ts": true,
    "packages/*/src/**/*.js": true,
    "packages/*/test/**/*.d.ts": true,
    "packages/*/test/**/*.js": true,
    "packages/js-examples/**/*.capnp.ts": true
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
